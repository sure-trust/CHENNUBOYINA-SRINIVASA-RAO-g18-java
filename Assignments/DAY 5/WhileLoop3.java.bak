import java.util.Scanner;
class WhileLoop3
{
	public static void main(String args[]){
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a integer number");
		int num=sc.nextInt();
		System.out.println("the first "=num+" even numbers");
		int i=1;
		while(i<=num){
			if(i%2==0)
				System.out.println(num);
			
			i++;
		}
	}
	    
		
}
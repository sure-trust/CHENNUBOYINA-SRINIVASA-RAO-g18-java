class Swaping_Of_3Numbers
{
	public static void main(String args[])
	{
		 int num1=10;
		 int num2=20;
		 int num3=30;
		 System.out.println("before swaping "+num1+" "+num2+" "num3);
		 num1=num1+num2+num3;
		 num2=num1-(num2+num3);
		 num3=num1-(num2+num3);
		 num1=num1-(num2+num3);
		 System.out.println("after swaping "+num1+" "+num2+" "+num3);
	}
}